<h1>Welcome,
    <%= @user_info['display_name'] %>
</h1>
<h4>
    <%= link_to "Logout", logout_path, method: :delete%>
</h4>
<h2>
    <%= @user_info['email']%>
</h2>
<h2>
    <%= link_to  "Spotify", @user_info['href']%>
</h2>

<img src=<%=@user_info[ 'images'][0][ 'url']%>>

<h2>Playlists:</h2>

<ul>
    <% @user_playlists.each do |playlist| %>
        <% unless playlist['owner']['id'] == current_user.uid %>
            <li>
                <%= link_to playlist['name'], playlist['external_urls']['spotify'] %>
            </li>
            <%end%>
                <%end%>
</ul>